! URxvt
URxvt*termName:		xterm

! Xft
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1 
Xft.rgba: rgb

! X11
Xcursor.theme: default

! urxvt, Config
URxvt*geometry:        	80x20
URxvt*font:  			xft:Liberation Mono:bold:size=8:hinting=full
URxvt*foreground:     	#ffffff
URxvt*borderless: 		0
URxvt*scrollBar:       	false
URxvt*saveLines: 		32767
URxvt.cursorBlink:		true
URxvt.iconFile: 		/usr/share/icons/Faenza/apps/scalable/terminal.svg
! Rxvt.scrollTtyOutput: false
! perl ext
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1

! remove tab shortcuts
URxvt.keysym.Shift-Right:       \033[1;2C
URxvt.keysym.Shift-Left:        \033[1;2D

URxvt*meta8:False

! True transparency
URxvt*depth: 32
URxvt*background: rgba:1111/1111/1111/aaaa

